{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "skipLibCheck": true,
    "moduleDetection": "force",
    "strictNullChecks": true,
    "types": ["vite/client", "bun-types"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
